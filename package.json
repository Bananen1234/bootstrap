{
  "name": "bootstrap-demo",
  "version": "1.0.0-alpha.5",
  "homepage": "https://sebgroup.github.io/bootstrap/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --aot --base-href /bootstrap/",
    "prebuild": "replace __TRAVIS_BUILD_NUMBER__ $TRAVIS_BUILD_NUMBER src/environments/environment.ts && replace __TRAVIS_BUILD_NUMBER__ $TRAVIS_BUILD_NUMBER src/environments/environment.prod.ts",
    "build-lib": "npm run copy-scss && npm run copy-fonts && npm run compile-css && copyfiles -f CHANGELOG.md LICENSE.md scss/package.json dist/lib",
    "prebuild-lib": "rimraf dist/lib",
    "prepublish-lib": "npm run build-lib",
    "publish-lib": "npm publish dist/lib",
    "build-css": "node-sass --importer=node_modules/node-sass-tilde-importer --source-map true --include-path scss scss/bootstrap-prebuild.scss dist/lib/css/bootstrap-prebuild.css",
    "autoprefix-css": "postcss -u autoprefixer -r dist/lib/css/bootstrap-prebuild.css",
    "minify-css": "cleancss -o dist/lib/css/bootstrap-prebuild.min.css dist/lib/css/bootstrap-prebuild.css --source-map",
    "build-all-css": "node-sass --importer=node_modules/node-sass-tilde-importer --source-map true --include-path scss scss/bootstrap-prebuild-all.scss dist/lib/css/bootstrap-prebuild-all.css",
    "autoprefix-all-css": "postcss -u autoprefixer -r dist/lib/css/bootstrap-prebuild-all.css",
    "minify-all-css": "cleancss -o dist/lib/css/bootstrap-prebuild-all.min.css dist/lib/css/bootstrap-prebuild-all.css --source-map",
    "rename-css": "renamer --find \"p-prebuild\" --replace \"p\" dist/lib/css/*",
    "compile-css": "npm run build-css && npm run autoprefix-css && npm run minify-css && npm run build-all-css && npm run autoprefix-all-css && npm run minify-all-css && npm run rename-css",
    "copy-scss": "copyfiles -u 1 scss/**/*.scss dist/lib/scss && rimraf dist/lib/scss/bootstrap-prebuild.scss dist/lib/scss/bootstrap-prebuild-all.scss",
    "copy-fonts": "copyfiles -u 2 scss/fonts/* dist/lib/fonts",
    "test": "ng test",
    "test-ci": "ng test --watch=false --progress --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.2",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "autoprefixer": "^9.1.0",
    "bootstrap": "4.1.3",
    "clean-css-cli": "^4.2.0",
    "codelyzer": "~4.2.1",
    "commitizen": "^2.10.1",
    "copyfiles": "^2.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass-tilde-importer": "^1.0.2",
    "postcss-cli": "^6.0.0",
    "prismjs": "^1.15.0",
    "protractor": "~5.3.0",
    "raw-loader": "^0.5.1",
    "renamer": "^1.0.0",
    "replace": "^1.0.0",
    "rimraf": "^2.6.2",
    "standard-version": "^4.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
