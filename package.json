{
  "name": "bootstrap-demo",
  "version": "1.0.0",
  "homepage": "https://sebgroup.github.io/bootstrap/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href https://sebgroup.github.io/bootstrap/ && copyfiles -f src/404.html dist/bootstrap",
    "prebuild": "rimraf dist/bootstrap && replace __TRAVIS_BUILD_NUMBER__ $TRAVIS_BUILD_NUMBER src/environments/environment.ts && replace __TRAVIS_BUILD_NUMBER__ $TRAVIS_BUILD_NUMBER src/environments/environment.prod.ts",
    "build-lib": "npm run copy-scss && npm run copy-fonts && npm run compile-css && copyfiles -f CHANGELOG.md LICENSE.md README.md scss/package.json dist/lib",
    "prebuild-lib": "rimraf dist/lib",
    "prepublish-lib": "npm run build-lib",
    "publish-lib": "npm publish dist/lib",
    "build-css": "node-sass --importer=node_modules/node-sass-tilde-importer --source-map true --include-path scss scss/bootstrap-prebuild.scss dist/lib/css/bootstrap-prebuild.css",
    "autoprefix-css": "postcss -u autoprefixer -r dist/lib/css/bootstrap-prebuild.css",
    "minify-css": "cleancss -o dist/lib/css/bootstrap-prebuild.min.css dist/lib/css/bootstrap-prebuild.css --source-map",
    "build-all-css": "node-sass --importer=node_modules/node-sass-tilde-importer --source-map true --include-path scss scss/bootstrap-prebuild-all.scss dist/lib/css/bootstrap-prebuild-all.css",
    "autoprefix-all-css": "postcss -u autoprefixer -r dist/lib/css/bootstrap-prebuild-all.css",
    "minify-all-css": "cleancss -o dist/lib/css/bootstrap-prebuild-all.min.css dist/lib/css/bootstrap-prebuild-all.css --source-map",
    "rename-css": "renamer --find \"p-prebuild\" --replace \"p\" dist/lib/css/*",
    "compile-css": "npm run build-css && npm run autoprefix-css && npm run minify-css && npm run build-all-css && npm run autoprefix-all-css && npm run minify-all-css && npm run rename-css",
    "copy-scss": "copyfiles -u 1 'scss/**/*.scss' -e 'scss/**/*-prebuild*.scss' dist/lib/scss",
    "copy-fonts": "copyfiles -u 2 scss/fonts/* dist/lib/fonts",
    "test": "ng test",
    "test-ci": "ng test --watch=false --progress --browsers=ChromeHeadlessNoSandbox",
    "visualtest:ci": "start-server-and-test start http://localhost:4200 visualtest",
    "visualtest": "backstop test",
    "visualtest:approve": "backstop approve",
    "add-fa-config": "npm config set '@fortawesome:registry' https://npm.fontawesome.com/ && npm config set '//npm.fontawesome.com/:_authToken' $FA_TOKEN",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "commit": "git-cz",
    "travis-deploy-once": "travis-deploy-once --pro",
    "semantic-release": "semantic-release && npm run build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "pkgRoot": "dist/lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.6",
    "@angular/common": "^6.1.6",
    "@angular/compiler": "^6.1.6",
    "@angular/core": "^6.1.6",
    "@angular/forms": "^6.1.6",
    "@angular/http": "^6.1.6",
    "@angular/platform-browser": "^6.1.6",
    "@angular/platform-browser-dynamic": "^6.1.6",
    "@angular/router": "^6.1.6",
    "@fortawesome/angular-fontawesome": "^0.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/pro-light-svg-icons": "^5.3.1",
    "@ng-bootstrap/ng-bootstrap": "^3.2.0",
    "@sebgroup/fonts": "^1.0.0",
    "angular-exemplify": "^3.1.1",
    "core-js": "^2.5.4",
    "rxjs": "^6.3.1",
    "swe-validation": "^1.0.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.5",
    "@angular/cli": "^6.1.5",
    "@angular/compiler-cli": "^6.1.6",
    "@angular/language-service": "^6.1.6",
    "@fortawesome/fontawesome-pro": "^5.3.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.29",
    "autoprefixer": "^9.1.3",
    "backstopjs": "^3.5.10",
    "bootstrap": "4.1.3",
    "clean-css-cli": "^4.2.1",
    "codelyzer": "~4.2.1",
    "commitizen": "^2.10.1",
    "copyfiles": "^2.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eyes.selenium": "0.0.83",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass-tilde-importer": "^1.0.2",
    "postcss-cli": "^6.0.0",
    "prismjs": "^1.15.0",
    "protractor": "^5.4.0",
    "raw-loader": "^0.5.1",
    "renamer": "^1.0.0",
    "replace": "^1.0.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.12",
    "start-server-and-test": "^1.7.1",
    "travis-deploy-once": "^5.0.7",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebgroup/bootstrap.git"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
