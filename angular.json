{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bootstrap": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./webpack.partial.js"
            },
            "aot": true,
            "outputPath": "dist/bootstrap",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "src/example/alerts/icons/icons.component.scss",
              "src/example/animations/animations.component.scss",
              "src/example/badge/dismissable/dismissable.component.scss",
              "src/example/badge/standard/standard.component.scss",
              "src/example/button-group/sizes/sizes.component.scss",
              "src/example/button-group/standard/standard.component.scss",
              "src/example/button/button.component.scss",
              "src/example/cards/columns/columns.component.scss",
              "src/example/checkboxes-and-slide-toggles/highlight-error/highlight-error.component.scss",
              "src/example/modals/scroll/scroll.component.scss",
              "src/example/modals/standard/standard.component.scss",
              "src/example/skeleton-loader/advanced/advanced.component.scss",
              "src/example/skeleton-loader/form/form.component.scss",
              "src/example/spinner/button/button.component.scss",
              "src/example/spinner/standard/standard.component.scss",
              "src/example/tables/horizontal-scroll/horizontal-scroll.component.scss",
              "src/example/tooltip/standard/standard.component.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "bootstrap:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "bootstrap:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bootstrap:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        }
      }
    },
    "bootstrap-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "bootstrap:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "bootstrap:serve:production"
            }
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "cli": {
    "analytics": false
  }
}
